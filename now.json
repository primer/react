{
  "name": "primer-components",
  "version": 2,
  "alias": "primer-components.now.sh",
  "routes": [
    {"src": "/playroom(/.*)?", "dest": "$1"},
    {"src": "/components(/.*)?", "dest": "/docs$1"},
    {
      "src": "/",
      "status": 301,
      "headers": {"Location": "/components"}
    }
  ],
  "builds": [
    {
      "src": "docs/package.json",
      "use": "@now/static-build",
      "config": {"distDir": "public"}
    },
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {"distDir": "public"}
    }
  ]
}
