    "typescript-plugin-css-modules": "^5.2.0",
    "unist-util-find": "3.0.0",
    "unist-util-find-before": "4.0.0",
    "unist-util-flat-filter": "2.0.0",
    "yaml": "2.7.0"
  },
  "peerDependencies": {
    "@types/react": "18.x || 19.x",
    "@types/react-dom": "18.x || 19.x",
    "@types/react-is": "18.x || 19.x",
    "react": "18.x || 19.x",
