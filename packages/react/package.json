    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-is": "18.3.1",
    "recast": "0.23.7",
    "rimraf": "5.0.5",
    "rollup": "4.53.3",
    "rollup-plugin-import-css": "^0.0.0",
    "rollup-plugin-postcss": "4.0.2",
    "rollup-plugin-visualizer": "6.0.5",
    "storybook": "^10.1.4",
    "terser": "5.36.0",
    "ts-toolbelt": "9.6.0",
    "tsx": "4.20.3",
